package {	import flash.display.MovieClip;	import flash.sensors.Accelerometer;	import flash.events.Event;	public class DocumentClass extends MovieClip {		private var factory:Factory;		public function DocumentClass() {		}		public function startFactory() {			stage.focus = stage;			var order:Array = new Array();			for (var i=0; i<10; i++) {				order.push(Item.getRandomItem());			}			this.factory = new Factory(this,order);						addEventListener(Event.ENTER_FRAME, this.processFactory);		}				private function processFactory(event:Event) {			if (!this.factory.process()) {				this.stopFactory();			}		}				private function stopFactory() {			removeEventListener(Event.ENTER_FRAME, this.processFactory);			this.factory = null;			this.gotoAndPlay(1, 'Game Over');		}	}}