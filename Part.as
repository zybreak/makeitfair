package  {	import flash.display.MovieClip;		public class Part extends MovieClip {				private var type:uint;		public function Part(type:uint) {			this.type = type;		}				public function resetPosition() {			this.x = 0;			this.y = -130;		}				public function isType(type:uint) {			if (type == this.type) {				this.dispatchEvent(new PartEvent(PartEvent.HIT));			} else {				this.dispatchEvent(new PartEvent(PartEvent.MISS));			}		}				public function process() {			this.y +=  5;			if (this.y > 100) {				this.dispatchEvent(new PartEvent(PartEvent.MISS));			}		}	}	}