package  {	import flash.display.MovieClip;		public class Part extends MovieClip {				private var type:uint;		private var yMin:int = -130;		private var yMax:int = 100;		private var yDiff:int = 5;		public function Part(type:uint) {			this.type = type;		}				public function resetPosition() {			this.x = 0;			this.y = this.yMin;		}				public function isType(type:uint) {			if (type == this.type) {				var points:uint = this.points();				this.dispatchEvent(new PartEvent(PartEvent.HIT, points));			} else {				this.dispatchEvent(new PartEvent(PartEvent.MISS));			}		}				public function process() {			this.y +=  this.yDiff;			if (this.y > this.yMax) {				this.dispatchEvent(new PartEvent(PartEvent.MISS));			}		}				private function points() {			var total:uint = this.yMax + Math.abs(this.yMin);			var progress:uint = Math.abs(this.yMin) + this.y;						return 100 - ((progress / total) * 100);		}	}	}