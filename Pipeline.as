package  {	import flash.events.Event;	import flash.display.MovieClip;		public class Pipeline extends MovieClip {				private var down:Down;		private var up:Up;		private var left:Left;		private var right:Right;				private var pipeline:Array;		private var failSound:FailSound;		public function Pipeline() {			this.pipeline = new Array();			this.failSound = new FailSound();						this.down = new  Down();			this.up = new Up();			this.left = new Left();			this.right = new Right();						addChild(this.down);		}				public function addItem(item) {			this.pipeline.push(item);		}				public function getNextItem() {			return this.pipeline.pop();		}				public function hasMoreItems() {			return this.pipeline.length > 0;		}				public function process(event:Event) {			this.down.y +=  5;			if (this.down.y > 100)			{				this.down.y = -130;				this.failSound.play();			}		}	}	}